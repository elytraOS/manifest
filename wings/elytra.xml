<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- GitLab -->
  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote  name="elytra"
           fetch="https://github.com/elytraOS/"
           revision="trident" />

  <remote  name="awaken-gitlab"
           fetch="https://gitlab.com/Project-Awaken/"
           revision="triton" />

  <!-- Rootdir -->
  <project path="art" name="art" groups="pdk" remote="elytra" />
  <project path="bionic" name="bionic" groups="pdk" remote="elytra" />
  <project path="manifest" name="manifest" remote="elytra" />

  <!-- Build -->
  <project path="build/make" name="build" groups="pdk" remote="elytra" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="elytra" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/elytra/sepolicy" name="device_elytra_sepolicy" remote="elytra" />
  <project path="device/qcom/sepolicy_vndr" name="device_qcom_sepolicy_vndr" groups="qcom,waipio-vendor" remote="elytra" />
  <project path="device/qcom/sepolicy_vndr-legacy-um" name="device_qcom_sepolicy-legacy-um" groups="qcom,lahaina" remote="elytra" />

  <!-- External -->
  <project path="external/tinyxml" name="platform/external/tinyxml" groups="pdk" remote="aosp" revision="refs/tags/android-11.0.0_r46" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="frameworks_av" groups="pdk" remote="elytra" />
  <project path="frameworks/base" name="frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="elytra" />
  <project path="frameworks/native" name="frameworks_native" groups="pdk-cw-fs,pdk-fs" remote="elytra" />
  <project path="frameworks/libs/systemui" name="frameworks_libs_systemui" groups="pdk-cw-fs,pdk-fs" remote="elytra" />

  <!-- Packages -->
  <project path="packages/apps/Covers" name="packages_apps_Covers" remote="elytra" />
  <project path="packages/apps/Dialer" name="packages_apps_Dialer" groups="pdk-fs" remote="elytra" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="elytra" />
  <project path="packages/apps/SettingsIntelligence" name="packages_apps_SettingsIntelligence" groups="pdk-fs" remote="elytra" />
  <project path="packages/apps/SimpleDeviceConfig" name="packages_apps_SimpleDeviceConfig" remote="elytra" />
  <project path="packages/apps/Launcher3" name="packages_apps_Launcher3" groups="pdk-fs" remote="elytra" />
  <project path="packages/apps/ThemePicker" name="packages_apps_ThemePicker" groups="pdk-fs" remote="elytra" />
  <project path="packages/modules/Connectivity" name="packages_modules_Connectivity" groups="pdk" remote="elytra" />
  <project path="packages/modules/Permission" name="packages_modules_Permission" groups="pdk-cw-fs,pdk-fs" remote="elytra" />
  <project path="packages/modules/Wifi" name="packages_modules_Wifi" groups="pdk-cw-fs,pdk-fs" remote="elytra" />
  <project path="packages/providers/DownloadProvider" name="packages_providers_DownloadProvider" groups="pdk-cw-fs,pdk-fs" remote="elytra" />
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" groups="pdk-cw-fs,pdk-fs" remote="elytra" />

  <!-- System -->
  <project path="system/core" name="system_core" groups="pdk" remote="elytra" />
  <project path="system/sepolicy" name="system_sepolicy" groups="pdk" remote="elytra" />

  <!-- Vendor -->
  <project path="vendor/elytra" name="vendor_elytra" remote="elytra" />
  <project path="vendor/gms" name="android_vendor_gms" remote="awaken-gitlab" clone-depth="1" />

  <!-- External -->
  <project path="external/AndroidSystemBlur" name="AndroidSystemBlur" remote="elytra" />
  <project path="external/arm-optimized-routines" name="external_arm-optimized-routines" remote="elytra" />
  <project path="external/ComputeLibrary" name="external_ComputeLibrary" remote="elytra" />
  <project path="external/f2fs-tools" name="external_f2fs-tools" remote="elytra" />
  <project path="external/giflib" name="external_giflib" remote="elytra" />
  <project path="external/harfbuzz_ng" name="external_harfbuzz_ng" remote="elytra"/>
  <project path="external/libjpeg-turbo" name="external_libjpeg-turbo" remote="elytra" />
  <project path="external/libpng" name="external_libpng" remote="elytra" />
  <project path="external/lz4" name="external_lz4" remote="elytra" />
  <project path="external/scudo" name="external_scudo" remote="elytra" />
  <project path="external/sqlite" name="external_sqlite" remote="elytra" />
  <project path="external/zstd" name="external_zstd" remote="elytra" />
</manifest>
